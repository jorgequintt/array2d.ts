{
  "name": "array2d",
  "version": "0.0.5",
  "description": "Utility library for working with 2D arrays (arrays of arrays) in JavaScript",
  "main": "./dist/Array2D.mjs",
  "module": "./dist/Array2D.js",
  "types": "./dist/Array2D.d.ts",
  "exports": {
    ".": {
      "import": "./dist/Array2D.js",
      "require": "./dist/Array2D.mjs",
      "types": "./dist/Array2D.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewtoast/Array2D.js.git"
  },
  "scripts": {
    "build": "tsup Array2D.js --format iife,esm,cjs --minify --clean"
  },
  "author": "Matthew Trost",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.3.4",
    "gulp-rename": "^1.2.0",
    "gulp-uglify": "^0.3.1",
    "mocha": "^1.21.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
